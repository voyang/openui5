<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:cw="urn:import:carapace.web2.gwt.client.basic.common.widgets"
    xmlns:aacw="urn:import:carapace.web2.addons.caracrm.gwt.client.aisyc.basic.automation.core.widgets"
    xmlns:aagw="urn:import:carapace.web2.addons.caracrm.gwt.client.aisyc.basic.automation.generic.widgets"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:p="urn:import:carapace.web2.addons.caracrm.gwt.client.crm.screen.widgets.customer.person">
    <ui:style>
    </ui:style>
    <aacw:AisycContainer fluid="true" ui:field="aisycBlockBase_layoutRoot" b:id="hideMultiMailAddrListBlockUI_layoutRoot">
        <cw:ExtHTMLPanelGWT styleName="form-horizontal ">
            <cw:ExtRowGBS3 styleName="form-group" marginTop="10">
                <cw:ExtColumnGBS3 size="COL_3">
                    <cw:ExtButtonGBS3 ui:field="hideMultiMailAddrListBlockUI_insertBtn" wordProperty="add=core.fixword.common.btn.add" styleName="btn btn-decall">
                        <i class="glyphicon glyphicon-plus">[add]</i>
                    </cw:ExtButtonGBS3>
                </cw:ExtColumnGBS3>
                <cw:ExtColumnGBS3 size="COL_3">
                    <cw:ExtButtonGBS3
                        ui:field="hideMultiMailAddrListBlockUI_saveBtn" wordProperty="save=core.fixword.common.btn.save" styleName="btn btn-decall">
                        <i class="glyphicon glyphicon-floppy-disk">[save]</i>
                    </cw:ExtButtonGBS3>
                </cw:ExtColumnGBS3>
                <cw:ExtColumnGBS3 size="COL_6">
                    <cw:ExtPaginationGBS3 styleName="pagination-decall" pageSize="5" paginationSize="SMALL" b:id="hideMultiMailAddrListBlockUI_pager"></cw:ExtPaginationGBS3>
                </cw:ExtColumnGBS3>
            </cw:ExtRowGBS3>
            <cw:ExtRowGBS3  styleName="form-group">
                <cw:ExtColumnGBS3 size="COL_12">
                    <aagw:AisycDataGrid height="200px" width="100%"
                    ui:field="hideMultiMailAddrListBlockUI_mailGrid"
                    embedPoint="複数メールアドレス情報_メールアドレス一覧"
                    pagerWidgetPath="...hideMultiMailAddrListBlockUI_layoutRoot/**hideMultiMailAddrListBlockUI_pager"></aagw:AisycDataGrid>
                </cw:ExtColumnGBS3>
            </cw:ExtRowGBS3>
            <cw:ExtRowGBS3 styleName="form-group">
                <cw:ExtColumnGBS3 size="COL_2">
                    <cw:ExtButtonGBS3 ui:field="hideMultiMailAddrListBlockUI_deleteBtn" wordProperty="delete=core.fixword.common.btn.delete" styleName="btn btn-decall">
                        <i class="glyphicon glyphicon-remove">[delete]</i>
                    </cw:ExtButtonGBS3>
                </cw:ExtColumnGBS3>
                <cw:ExtColumnGBS3 size="COL_2">
                    <cw:ExtButtonGBS3
                        ui:field="hideMultiMailAddrListBlockUI_upBtn" text="↑" styleName="btn btn-decall"></cw:ExtButtonGBS3>
                </cw:ExtColumnGBS3>
                <cw:ExtColumnGBS3 size="COL_2">
                    <cw:ExtButtonGBS3
                        ui:field="hideMultiMailAddrListBlockUI_downBtn" text="↓" styleName="btn btn-decall"></cw:ExtButtonGBS3>
                </cw:ExtColumnGBS3>
                <cw:ExtColumnGBS3 size="COL_3">
                    <cw:ExtButtonGBS3
                        ui:field="hideMultiMailAddrListBlockUI_maiBtn" wordProperty="createmail=caracrm.fixword.cust_person.subwindow.cust_contact_list.btn.create_mail" styleName="btn btn-decall">
                        <i class="glyphicon glyphicon-envelope">[createmail]</i>
                    </cw:ExtButtonGBS3>
                </cw:ExtColumnGBS3>
                <cw:ExtColumnGBS3 size="COL_3">
                </cw:ExtColumnGBS3>
            </cw:ExtRowGBS3>
        </cw:ExtHTMLPanelGWT>
    </aacw:AisycContainer>
</ui:UiBinder>